#include <iostream>
#include <cmath>
#include <vector>

std::vector<double> nestedSum( std::vector<double>& p, double z )
{
	std::vector<double> p_;
	p_.push_back( p[p.size()-1] );
	std::cout << p.size() << std::endl;
	//for ( int i=p.size(); i<p.size(); i++ )
	//{}

	return p_;
}

int main( int argc, char *argv[] )
{
	std::vector<double> p;
	for ( int i=0; i<10; i++ )
		p.push_back( i );

	nestedSum( p, 0.0 );

	return 0;
}
